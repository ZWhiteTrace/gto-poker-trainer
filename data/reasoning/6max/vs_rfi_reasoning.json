      "A5s": {
        "role": "mix_3bet_fold",
        "tags": ["ace_blocker", "wheel_potential", "polarized_range_bluff", "suited"],
        "compare_group": "suited_aces",
        "stability": "stable",
        "note": "經典 3bet bluff 候選：Ace blocker + wheel potential + 同花。比 A4s 多一個順子組合。"
      },
      "A4s": {
        "role": "mix_3bet_fold",
        "tags": ["ace_blocker", "wheel_potential", "polarized_range_bluff"],
        "compare_group": "suited_aces",
        "stability": "param_sensitive",
        "note": "A4s 是 3bet bluff 的邊界。比 A5s 少一個順子組合 (A2345)，部分 solver 將其設為 fold。"
      },
      "KQs": {
        "role": "mix_call_3bet",
        "tags": ["high_equity", "high_eqr_suited", "nut_potential", "connectivity"],
        "compare_group": "suited_broadways",
        "stability": "param_sensitive",
        "note": "KQs 在 CO 可以作為 thin value 3bet（面對 UTG call range 有高 equity），也可以 call 保護 condensed range。"
      },
      "AQo": {
        "role": "mix_call_3bet",
        "tags": ["high_equity", "domination_risk", "linear_range_top"],
        "compare_group": "premium_broadways",
        "stability": "param_sensitive",
        "note": "CO 比 HJ 位置好，AQo 混合策略出現。3bet 可以打跑 TT-99/AJs 等，但被 4bet 時很尷尬。"
      },
      "AKo": {
        "role": "value_3bet",
        "tags": ["high_equity", "ace_blocker", "nut_potential"],
        "compare_group": "premium_broadways",
        "stability": "stable"
      }
    }
  },
